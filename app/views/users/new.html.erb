<h1>Sign Up</h1>

<%= form_for @user do |f| %>
	<fieldset>
		<%= f.label :email %>
		<%= f.text_field :email %>
	</fieldset>
	<fieldset>
		<%= f.label :password %>
		<%= f.password_field :password %>
	</fieldset>
	<fieldset>
		<%= f.label :password_confirmation %>
		<%= f.password_field :password_confirmation %>
	</fieldset>
	<%= f.submit "Sign Up" %>
<% end %>